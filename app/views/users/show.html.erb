<h1>Your Bids</h1>
<table class='table table-hover'>
<tr>
  <th>Auction Title</th>
  <th>Current Price</th>
  <th>Your Bid</th>
  <th>Bid Placed</th>
</tr>
<% @bids.each do |b| %>
<tr>
  <td><%= link_to b.auction.title, auction_path(b.auction) %></td>
  <td><%= b.auction.current_price %></td>
  <td><%= b.bid %></td>
  <td><%= b.created_at %></td>
</tr>
<% end %>
</table>
